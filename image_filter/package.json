{
  "name": "wasm-js-benchmarks",
  "version": "1.0.0",
  "description": "Performance comparison between WebAssembly and JavaScript implementations",
  "main": "server.js",
  "scripts": {
    "server": "node server.js",
    "server:8080": "node server.js --port 8080",
    "benchmark": "node benchmark-cli.js",
    "benchmark:mandelbrot": "node benchmark-cli.js --benchmark mandelbrot",
    "benchmark:matrix": "node benchmark-cli.js --benchmark matrix",
    "benchmark:fft": "node benchmark-cli.js --benchmark fft",
    "benchmark:iteration": "node benchmark-cli.js --benchmark iteration",
    "benchmark:memory": "node benchmark-cli.js --benchmark memory",
    "benchmark:primes": "node benchmark-cli.js --benchmark primes",
    "benchmark:fib": "node benchmark-cli.js --benchmark fib",
    "benchmark:all": "node benchmark-cli.js --iterations 3",
    "benchmark:export": "node benchmark-cli.js --iterations 5 --export results.json",
    "benchmark:verbose": "node benchmark-cli.js --verbose",
    "wasm:build": "cd wasm_app && wasm-pack build --release && cd ..",
    "wasm:dev": "cd wasm_app && wasm-pack build && cd .."
  },
  "keywords": [
    "wasm",
    "benchmark",
    "performance",
    "javascript",
    "comparison"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {}
}
